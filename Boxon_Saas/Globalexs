		String []trackingid= {"Ram165","Ram166","Ram167","Ram168","Ram169","Ram170"};
		WebElement searchbar=driver.findElement(By.id("txtTrackNo"));
		Thread.sleep(5000);
		for (int i = 0; i < trackingid.length; i++) {
			String string = trackingid[i];
		}
        for(String trackingids : trackingid) {
        	searchbar.clear();
			searchbar.sendKeys(trackingids);
//			searchbar.sendKeys(Keys.RETURN);
			System.out.println(trackingids);
        }
				if( driver.findElements(By.xpath("//*[@id=\"lblFinalCost\"]	")).equals("0")){
					System.out.println("Please enter Grossweight");
					driver.findElement(By.id("txtmerchantname")).click();
	        		JavascriptExecutor js=(JavascriptExecutor)driver;
	        		js.executeScript("window.scrollBy(0,650)","");
//	        		driver.findElement(By.id("txtmerchantname")).click();
