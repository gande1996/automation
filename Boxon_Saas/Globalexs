package com.ible.globalexs;

import java.time.Duration;
import java.util.Arrays;
import java.util.Iterator;
import java.util.LinkedList;
import java.util.List;
import java.util.Set;

import org.openqa.selenium.By;
import org.openqa.selenium.JavascriptExecutor;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.chrome.ChromeOptions;
import org.openqa.selenium.support.ui.Select;

public class Test54 {

	public static void main(String[] args) throws InterruptedException {
		ChromeOptions co=new  ChromeOptions();
		co.addArguments("--remote-allow-origins=*");
		WebDriver driver = new ChromeDriver(co);
		driver.manage().window().maximize();
		driver.get("https://manage.smarteweb.com/");
		driver.findElement(By.id("ctl00_ContentPlaceHolder1_txtusername")).sendKeys("gabi");
		driver.findElement(By.id("ctl00_ContentPlaceHolder1_txtpassword")).sendKeys("Boxon@123");
		driver.findElement(By.id("ctl00_ContentPlaceHolder1_btnlogin")).click();
		Thread.sleep(2000);
		driver.findElement(By.id("ctl00_lnkQuickActions")).click();
		driver.findElement(By.id("ctl00_lnkBILL")).click();
		Thread.sleep(2000);
		driver.switchTo().frame("ctl00_ASPxPopupControl1_CIF-1");
		Thread.sleep(2000);
		List<String> trackingid=Arrays.asList("Task027","Task028","Task029","Task030");
		WebElement searchbar=driver.findElement(By.id("txtTrackNo"));
		Thread.sleep(5000);
        for(String trackingids : trackingid) {
        	searchbar.clear();
			searchbar.sendKeys(trackingids);
			searchbar.sendKeys(Keys.RETURN);
    		Thread.sleep(2000);
        	if ( driver.findElements(By.xpath("//*[@id=\"lblFinalCost\"]	")).equals("0")){
        		System.out.println("Please enter Grossweight");
        	}else if(driver.findElements(By.id("GrdMeasurement_ctl02_txtGrossWtPerItem")).size()>0){
        		driver.findElement(By.id("txtmerchantname")).click();
        		JavascriptExecutor js=(JavascriptExecutor)driver;
        		js.executeScript("window.scrollBy(0,650)","");
//        		driver.findElement(By.id("txtmerchantname")).click();
        		Thread.sleep(5000);
        		Select dd1=new Select(driver.findElement(By.id("GrdMeasurement_ctl02_ddlStatus")));
        		dd1.selectByVisibleText("Arribo Barcelona");
        		driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(3));
        		driver.findElement(By.id("GrdMeasurement_ctl02_txtLength")).clear();
        		driver.findElement(By.id("GrdMeasurement_ctl02_txtLength")).sendKeys("1");
        		driver.findElement(By.id("GrdMeasurement_ctl02_txtWidth")).clear();
        		driver.findElement(By.id("GrdMeasurement_ctl02_txtWidth")).sendKeys("1");
        		driver.findElement(By.id("GrdMeasurement_ctl02_txtHeight")).clear();
        		driver.findElement(By.id("GrdMeasurement_ctl02_txtHeight")).sendKeys("1");
        		driver.findElement(By.id("GrdMeasurement_ctl02_txtGrossWtPerItem")).sendKeys("2");
        		Thread.sleep(2000);
        		driver.findElement(By.id("btnCal")).click();
        		Thread.sleep(5000);
        		driver.findElement(By.id("btnaddandnew")).click();
        		Thread.sleep(2000);
        		Set<String> instancewindow= driver.getWindowHandles();
                Iterator<String> it = instancewindow.iterator();
                String parent =it.next();
                String child = it.next();
                driver.switchTo().window(child);
                driver.close();
                driver.switchTo().window(parent);
                driver.findElement(By.id("btnMailClose")).click();
                driver.findElement(By.xpath("//*[@id=\"ctl00_ASPxPopupControl1_HRB-1\"]/img")).click();
			}
		}
        try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
	}

}
